{% extends 'adminbase.html.twig' %}

{% block title %}Hotel{% endblock %}

{% block body %}
    <h1>Hotel</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ hotel.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ hotel.title }}</td>
            </tr>
            <tr>
                <th>Keywords</th>
                <td>{{ hotel.keywords }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ hotel.description }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ hotel.image }}</td>
            </tr>
            <tr>
                <th>Star</th>
                <td>{{ hotel.star }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ hotel.phone }}</td>
            </tr>
            <tr>
                <th>Fax</th>
                <td>{{ hotel.fax }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ hotel.email }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ hotel.city }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ hotel.country }}</td>
            </tr>
            <tr>
                <th>Location</th>
                <td>{{ hotel.location }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ hotel.status }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ hotel.createdAt ? hotel.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ hotel.updatedAt ? hotel.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>End</th>
                <td>{{ hotel.end }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_hotel_index') }}">back to list</a>

    <a href="{{ path('admin_hotel_edit', {'id': hotel.id}) }}">edit</a>

    {{ include('admin/hotel/_delete_form.html.twig') }}
{% endblock %}
